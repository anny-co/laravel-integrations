{
  "name": "anny/laravel-integrations",
  "description": "Integrations for laravel applications.",
  "keywords": [
    "laravel",
    "anny",
    "integrations",
    "plugins"
  ],
  "type": "library",
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "v6.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Anny",
      "email": "hello@anny.co"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/contracts": "^7.0|^8.0",
    "illuminate/container": "^7.0|^8.0",
    "illuminate/database": "^7.0|^8.0",
    "laravel/socialite": "^5.2",
    "cloudcreativity/laravel-json-api": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Anny\\Integrations\\": "src/",
      "Anny\\Integrations\\Tests\\": "tests"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Anny\\Integrations\\IntegrationsServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}